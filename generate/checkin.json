{
  "name": "checkin",
  "inherits": "base",
  "mappings": [
  
    {
      "mbean": "ness.checkin:name=queryWait,type=controller",
      ResponseStatistics QueryWait
    },
  
    {
      "mbean": "ness.checkin:name=storeWait,type=controller",
      ResponseStatistics StoreWait
    },
 
    {
      "mbean": "ness.checkin:name=checkinInserts,type=controller",
      "attributes": [
        {
          "name": "Count",
          "graph": {
            "name": "Insert_Count",
